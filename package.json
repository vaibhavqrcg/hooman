    "@mistralai/mistralai": "^1.5.2",
    "@openai/agents": "^0.4.4",
    "@qdrant/js-client-rest": "1.8.2",
    "@supabase/supabase-js": "^2.49.1",
    "cloudflare": "^4.2.0",
    "cors": "^2.8.5",
    "debug": "^4.3.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "groq-sdk": "0.3.0",
    "lucide-react": "^0.460.0",
    "mem0ai": "^2.2.2",
    "mongodb": "^6.21.0",
