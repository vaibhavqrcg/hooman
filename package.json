    "@langchain/core": "^1.1.19",
    "@mistralai/mistralai": "^1.14.0",
    "@openai/agents": "^0.4.5",
    "@qdrant/js-client-rest": "1.16.2",
    "@supabase/supabase-js": "^2.94.1",
    "cloudflare": "^5.2.0",
    "cors": "^2.8.6",
    "debug": "^4.4.3",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "groq-sdk": "^0.37.0",
    "lucide-react": "^0.563.0",
    "mem0ai": "^2.2.2",
    "mongodb": "^7.1.0",
    "neo4j-driver": "^6.0.1",
    "ollama": "^0.6.3",
    "openai": "^6.17.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
