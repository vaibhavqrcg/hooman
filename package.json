{
  "name": "hooman",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "build": "yarn build:api && yarn build:web",
    "build:api": "tsc -p apps/api",
    "build:web": "cd apps/web && vite build",
    "dev": "tsx watch apps/api/src/index.ts",
    "dev:web": "cd apps/web && vite",
    "dev:all": "concurrently \"yarn dev\" \"yarn dev:web\"",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@azure/identity": "^4.13.0",
    "@azure/search-documents": "^12.2.0",
